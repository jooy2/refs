{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"declaration": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true
	}
}
